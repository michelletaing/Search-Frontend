{
    "name": "sift.",
    "version": "1.0.0",
    "description": "Search with sift.",
    "manifest_version": 3,
    "author": "Michelle Taing",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting"
    ],
    "action":{
        "default_popup": "index.html",
        "default_title": "Michelle's Extension"
    },
    "background": {
        "service_worker": "background.js"
    }
}